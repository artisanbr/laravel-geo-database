{
    "name": "renalcio/laravel-geo-database",
    "description": "Laravel Global Geo Info Database",
    "require": {
        "php": ">=7",
        "laravel/framework": ">=5.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Renalcio Carlos",
            "email": "renalcio.c@gmail.com",
            "homepage": "https://github.com/renalcio",
            "role": "Co-founder & Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevApex\\LaravelGeoDatabase\\GeoDatabaseServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "DevApex\\LaravelGeoDatabase\\": "src"
        }
    }
}
